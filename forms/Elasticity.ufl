from ufl import *
import basix.ufl

element = basix.ufl.element("Lagrange", basix.CellType.$cell, $degree, shape=(3,))
mesh = Mesh(basix.ufl.element("Lagrange", basix.CellType.$cell, 1, shape=(3,)))

V = FunctionSpace(mesh, element)

u = TrialFunction(V)
v = TestFunction(V)

W = FunctionSpace(mesh, basix.ufl.element("Lagrange", basix.CellType.$cell, 1))
k = Coefficient(W)

def eps(v):
    return sym(grad(v))

a = k*inner(eps(u), eps(v))*dx

un = Coefficient(V)
L = action(a, un)