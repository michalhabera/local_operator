from ufl import *
import basix.ufl

element = basix.ufl.element("Lagrange", basix.CellType.$cell, $degree)
mesh = Mesh(basix.ufl.element("Lagrange", basix.CellType.$cell, 1, shape=(3,)))

V = FunctionSpace(mesh, element)
u = TrialFunction(V)
v = TestFunction(V)

a = inner(u, v)*dx

un = Coefficient(V)
L = action(a, un)